<#import "/common/app.ftl" as app> <#include "/common/common.ftl" /><!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta http-equiv="Content-Language" content="zh-CN">    <meta name="keywords" content="">    <meta name="description" content="">    <meta name="copyright" content="" />    <meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">    <meta content="yes" name="apple-mobile-web-app-capable">    <meta content="black" name="apple-mobile-web-app-status-bar-style">    <meta content="telephone=no" name="format-detection">    <title>创客联盟</title>    <!-- css -->    <link href="${app.basePath}/static/touch/css/common.css" rel="stylesheet" type="text/css" />    <link href="${app.basePath}/static/touch/css/main.css" rel="stylesheet" type="text/css" />    <link href="${app.basePath}/static/touch/css/f-personalcenter.css" rel="stylesheet" type="text/css" />    <!-- js -->    <script type="text/javascript" src="${app.basePath}/static/touch/js/jquery-1.9.1.min.js"></script>    <script type="text/javascript" src="${app.basePath}/static/touch/js/common.js"></script></head><body class="body_gray"><!-- header_top --><div class="top_heater">    <a href="${app.basePath}/mobile/user/center" title="返回个人中心" class="hleft hback"></a>    <span>联盟创客</span></div><!-- header_top end --><!-- Center Start --><section class="container chuangke-list">    <span class="top"><label>联盟创客</label><font>（<label id="totalCount">${subcount}</label>人）</font></span>    <img src="${app.basePath}/static/touch/images/lianmengchuangke.png" class="banner" alt=""/>	<!--1.3改-->    <div class="list">        <ul class="yiji">        	<#if subList??>            	<#list subList as subuser>            <li>                <section class="member inactive">                    <#if subuser.uavatar??>			      		<#if subuser.uavatar?index_of("http")==0>			        	<img src="${subuser.uavatar!''}" alt="暂无头像">			        	<#else>				        <img src="${app.basePath}${subuser.uavatar!''}" alt="暂无头像">				        </#if>				      <#else>				      <img src="${app.basePath}/static/default/mobile/images/defaultavtar.png" alt="暂无头像">				      </#if>                    <div>                        <div class="title">                            <span class="membername">${subuser.unick!''}&nbsp;</span>                            <span class="<#if subuser.ugenter?? && subuser.ugenter==1>man<#elseif subuser.ugenter?? && subuser.ugenter==2>woman<#else></#if>"></span>                        </div>                        <span>会员ID：${subuser.uname!''}</span>                        <span>出生日期 : <#if subuser.ubirthday??>${subuser.ubirthday?string('yyyy-MM-dd')}</#if></span>                    </div>                </section>                <#if subuser.children??>                <ul style="display: none">                <#list subuser.children as subuser2>                    <li class="last">                        <section class="member2">                            <#if subuser2.uavatar??>				      		<#if subuser2.uavatar?index_of("http")==0>				        	<img src="${subuser2.uavatar!''}" alt="暂无头像">				        	<#else>					        <img src="${app.basePath}${subuser2.uavatar!''}" alt="暂无头像">					        </#if>					      <#else>					      <img src="${app.basePath}/static/default/mobile/images/defaultavtar.png" alt="暂无头像">					      </#if>                            <div>                                <div class="title">                                    <span class="membername">${subuser2.unick!''}&nbsp;</span>                                    <span class="<#if subuser2.ugenter?? && subuser2.ugenter==1>man<#elseif subuser2.ugenter?? && subuser2.ugenter==2>woman<#else></#if>"></span>                                </div>                                <span>会员ID：${subuser2.uname!''}</span>                                <span>出生日期: <#if subuser2.ubirthday??>${subuser2.ubirthday?string('yyyy-MM-dd')}</#if></span>                            </div>                            <span class="grade3">2级</span>                        </section>                        <#if subuser2.children??>                        <ul>                		<#list subuser2.children as subuser3>                            <li>                                <section class="member3">                                    <#if subuser3.uavatar??>							      		<#if subuser3.uavatar?index_of("http")==0>							        	<img src="${subuser3.uavatar!''}" alt="暂无头像">							        	<#else>								        <img src="${app.basePath}${subuser3.uavatar!''}" alt="暂无头像">								        </#if>								      <#else>								      <img src="${app.basePath}/static/default/mobile/images/defaultavtar.png" alt="暂无头像">								      </#if>                                    <div>                                        <div class="title">                                            <span class="membername">${subuser3.unick!''}&nbsp;</span>                                            <span class="<#if subuser3.ugenter?? && subuser3.ugenter==1>man<#elseif subuser3.ugenter?? && subuser3.ugenter==2>woman<#else></#if>"></span>                                        </div>                                        <span>会员ID：${subuser3.uname!''}</span>                                        <span>出生日期:<#if subuser3.ubirthday??>${subuser3.ubirthday?string('yyyy-MM-dd')}</#if></span>                                    </div>                                    <span class="grade3">3级</span>                                </section>                            </li>                		</#list>                		</ul>                		</#if>                    </li>                </#list>                 </ul>                </#if>            </li>            </#list>            </#if>        </ul>    </div></section><!-- Center End --><script>	$(document).ready(function(){	 	tree();	});</script></body></html>