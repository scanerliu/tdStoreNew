<#import "/common/app.ftl" as app> <script type="text/javascript">  var onOff = false;  <#if address?? && address.isDefault?? && address.isDefault==true>  onOff = true;  </#if>  $(function(){      $("#address").citySelect({          nodata:"none",          <#if province??>prov: "${province!''}",</#if>          <#if city??>city: "${city!''}",</#if>          <#if district??>dist: "${district!''}",</#if>          required:false      });    });  </script>    <div class="clear h35"></div>  <form id="submitForm" >	  <input type="hidden" id="addressId"  value="${address.id!''}" >	  <div class="info-list">	    <label>收件人</label>	    <input type="text" id="receiverName" placeholder="收货人姓名" value="${address.name!''}"/>	  </div>	  <div class="info-list">	    <label>联系人电话</label>	    <input type="text" id="receiverMobile" placeholder="收货人电话" value="${address.telphone!''}"/>	  </div>	  <div class="info-list" id="address">	    <label>所在地区</label>	    <select id="prov" name="provinceId" class="prov"></select>	    <select id="city" name="cityId" class="city"></select>	    <select id="dist" name="disctrictId" class="dist"></select>	    <div class="clear"></div>	    <textarea id="detailAddress" placeholder="详细地址">${address.address!''}</textarea>	  </div>	  <input class="btn-ok-modify" type="button" value="确定" onclick="save();" />	  <input class="btn-ok-modify" type="button" value="取消" onclick="backtoList();" /></form>