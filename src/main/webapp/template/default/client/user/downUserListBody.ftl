<#import "/common/app.ftl" as app> <ul class="active"><#if userList?? && userList?size gt 0>	<#list userList as item>		<li>		    <span class="isp isp${item.clevel!'3'}"></span>		    <dl>		      <dt>		      	  <#if item.uavatar??>			      		<#if item.uavatar?index_of("http")==0>			        	<img src="${item.uavatar!''}" alt="暂无头像">			        	<#else>				        <img src="${app.basePath}${item.uavatar!''}" alt="暂无头像">				        </#if>			      <#else>			      <img src="${app.basePath}/static/default/mobile/images/defaultavtar.png" alt="暂无头像">			      </#if>		      </dt>		      <dd style="margin-top:10px;">		        <label for="" class="lab1 fl">昵        称：</label>		        <span class="fl sp1">${item.unick!''}</span>		      </dd>		      <dd>		        <label for="" class="lab1 fl">性        别：</label>		        <span class="fl sp1">${item.ugenterStr!''}</span>		      </dd>		      <dd>		        <label for="" class="lab1 fl">用  户  名：</label>		        <span class="fl sp1">${item.uname!''}</span>		      </dd>		      <dd>		        <label for="" class="lab1 fl">注册时间：</label>		        <span class="fl sp1">${item.createTime?string('yyyy-MM-dd')}</span>		      </dd>		    </dl>		 </li>	</#list><#else>  <span class="fl">&nbsp;</span>  <span class="fl">暂无相关记录</span>  <span class="sp3 fl">&nbsp;</span></#if></ul><input type="hidden" value="${sc.clevelType!''}" name="clevelType"><#if userList?? && userList?size gt 0><div class="goods-page-nums mt20 w100 txtr">	<#assign pageId="DownUsers" />	<#include "../common/commonpostpage.ftl" /></div></#if>