<#import "/common/app.ftl" as app><!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta http-equiv="Content-Language" content="zh-CN">    <meta name="keywords" content="${system.webkeywords!''}">    <meta name="description" content="${system.webdescription!''}">    <meta name="copyright" content="${system.webcopyright!''}" />    <link rel="shortcut icon" href="${app.basePath}/static/default/images/icon.ico" />    <meta name="viewport" content="width=device-width,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no">    <meta content="yes" name="apple-mobile-web-app-capable">    <meta content="black" name="apple-mobile-web-app-status-bar-style">    <meta content="telephone=no" name="format-detection">    <title>创客联盟</title>    <!-- css -->    <link href="${app.basePath}/static/default/mobile/css/common.css" rel="stylesheet" type="text/css" />    <link href="${app.basePath}/static/default/mobile/css/main.css" rel="stylesheet" type="text/css" />    <link href="${app.basePath}/static/default/mobile/css/f-personalcenter.css" rel="stylesheet" type="text/css" />    <!-- js -->    <script type="text/javascript" src="${app.basePath}/static/touch/js/jquery-1.9.1.min.js"></script>    <script type="text/javascript" src="${app.basePath}/static/touch/js/common.js"></script>    <script type="text/javascript" src="${app.basePath}/static/js/mobile/product/search.js"></script></head><body class="body_gray"><script type="text/javascript">$(document).ready(function(){	var url = '${app.basePath}/mobile/user/comment/search'	$('#commone_list').refresh(url,"#commone_list",1);})</script><!-- header_top --><div class="top_heater">    <a href="javascript:history.go(-1);" title="" class="hleft hback"></a>    <span>我的评价</span></div><!-- header_top end --><!-- Center Start --><section class="container my-evaluate-list">    <ol>        <!-- 全部订单-->        <li id="commone_list">			<#if commentList??>        	<#list commentList as comm>            <section>                <div class="top">                    <p class="p1">订单号：<span><#if comm.order??>${comm.order.orderNo!''}</#if></span></p>                    <p class="p2">交易状态：<span>已评价</span></p>                </div>                <div class="middle">                    <img src="<#if comm.product??>${app.basePath}${comm.product.imageUrl!''}</#if>" alt="<#if comm.product??>${comm.product.name!''}</#if>"/>                    <div class="right-content">                        <h3><#if comm.product??>${comm.product.name!''}</#if></h3>                        <div>                            <#if comm.specialList??>			              	<#list comm.specialList as special>			              	<span><label for="">${special.sname!''}：</label><font>${special.soption!''}</font></span>			                </#list>			                </#if>                        </div>                        <p>￥168.00</p>                    </div>                </div>                <div class="bottom">                    共 <span><#if comm.order??>${comm.order.itemNum!'0'}</#if></span> 件商品  合计<strong class="price">￥<span><#if comm.order?? && comm.order.productAmount??>${comm.order.productAmount?string('0.00')}</#if></span></strong>（含运费：<strong>￥<span><#if comm.order?? && comm.order.postage??>${comm.order.postage?string('0.00')}</#if></span></strong>）                </div>                <div class="my-evaluate">                    <span>我的评价：</span>                    <div class="evaluate">                        <menu class="reviewsbox">                        	<#if comm.score??>                        	<#switch comm.score>								  <#case 1>								  	<span class="sel">&nbsp;</span>		                            <span>&nbsp;</span>		                            <span>&nbsp;</span>		                            <span>&nbsp;</span>		                            <span>&nbsp;</span>								  <#break>								  <#case 2>								  	<span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span>&nbsp;</span>		                            <span>&nbsp;</span>		                            <span>&nbsp;</span>								  <#break>								  <#case 3>								  	<span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span >&nbsp;</span>		                            <span>&nbsp;</span>								  <#break>								  <#case 4>								  	<span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span>&nbsp;</span>								  <#break>								  <#case 5>								  	<span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>		                            <span class="sel">&nbsp;</span>								  <#break>				        	</#switch>                        	</#if>                        </menu>                        <p>                            ${comm.content!''}                        </p>                    </div>                </div>            </section>         	</#list>         	</#if>        <!-- 已评价-->    </ol></section><!-- Center End --></body></html>