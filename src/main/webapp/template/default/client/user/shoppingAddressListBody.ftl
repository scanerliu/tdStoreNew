<#import "/common/app.ftl" as app>   <#if address_list?size gt 0>	  <#list address_list as address>	  <div class="address-list" id=${address.id!""}>        <div class="div1">          <p class="p1">            <span>${address.name!''}</span>            <span>${address.telphone!''}</span>            <a href="javascript:void(0)">默认地址</a>            <a href="<#if address.isDefault?? && address.isDefault==true>#<#else>${app.basePath}/mobile/user/shoppingAddress?addressId=${address.id}</#if>" title=""><span class="<#if address.isDefault?? && address.isDefault==true>default<#else>setdefault</#if>"><i></i><label for="">默认地址</label></span></a>	        <a href="${app.basePath}/mobile/user/shoppingAddressAdd?addressId=${address.id}" title="" class="a-edit">编辑</a>	        <a class="btn-swipe-delete">删除</a>          </p>          <p class="p2">${address.fullAddress!''}</p>        </div>      </div>	  </#list>				</#if>