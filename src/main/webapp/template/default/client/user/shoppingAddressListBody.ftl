<#import "/common/app.ftl" as app>   <#if address_list?size gt 0>	  <#list address_list as address>	  <div class="address-list" id=${address.id!""}>        <div class="div1">          <p class="p1">            <span>${address.name!''}</span>            <span>${address.telphone!''}</span>            <#if address.isDefault?? && address.isDefault==true>            	<a href="javascript:;">默认地址</a>            <#else>            	<a href="javascript:;" title="设为默认地址" onclick="defaultAddress(${address.id!""})">设为默认</a>            </#if>	        <a href="javascript:;" title="编辑地址" class="a-edit" onclick="editAddress(${address.id!""})">编辑</a>	        <a href="javascript:;" title="删除地址" class="btn-swipe-delete"  onclick="delAddress(${address.id!""})">删除</a>          </p>          <p class="p2">${address.fullAddress!''}</p>        </div>      </div>	  </#list>				</#if>